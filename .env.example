# .env.example - Template environment configuration file
# Copy this file to .env for local development
# DO NOT add actual secrets to this template file!

# Supabase Configuration
# In production, these values are provided by the Netlify serverless function
SERVER_SUPABASE_URL=https://api.manassistant.com
SERVER_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# DeepSeek AI Configuration (for memory and conversation enhancement)
SERVER_DEEPSEEK_API_KEY=your_deepseek_api_key_here
SERVER_DEEPSEEK_MODEL=deepseek-chat

# For development only - these will be provided by the serverless function in production
VITE_SUPABASE_URL=https://api.manassistant.com
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
VITE_DEEPSEEK_API_KEY=your_deepseek_api_key_here
VITE_DEEPSEEK_MODEL=deepseek-chat 